<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FacadeDMSService WSDL service -->

  <service name="FacadeDMS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://pckFacadeDms"/>
      <parameter name="wsdlServiceElement" value="FacadeDMSService"/>
      <parameter name="schemaQualified" value="http://pckFacadeDms"/>
      <parameter name="wsdlServicePort" value="FacadeDMS"/>
      <parameter name="className" value="pckFacadeDms.FacadeDMSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="FacadeDMS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="selectPhase" qname="operNS:selectPhase" xmlns:operNS="http://pckFacadeDms" returnQName="retNS:selectPhaseReturn" xmlns:retNS="http://pckFacadeDms" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:iSelectPhase" xmlns:pns="http://pckFacadeDms" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="selectPhase"/>

  </service>
</deployment>
